  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build": "cd android && ./gradlew assembleRelease && cd app/build/outputs/apk/release && ren app-release.apk new-pediupecas.apk && cd ../../../../../../",
    "clean": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd app/build/outputs/apk/release && ren app-release.apk new-pediupecas.apk && cd ../../../../../../",
    "re-build": "cd android/app/build/outputs/apk/release && del *.apk && cd ../../../../../ && ./gradlew assembleRelease && cd app/build/outputs/apk/release && ren app-release.apk new-pediupecas.apk && cd ../../../../../../",
    "justClean": "cd android && ./gradlew clean",
    "patch": "yarn version --patch",
    "end-sprint": "yarn version --minor",
    "release": "yarn version --major"
  },
  "dependencies": {
